##### Clock #####
NET  sys_clk_p_i LOC = K15;
NET  sys_clk_p_i IOSTANDARD = LVDS_25;

NET  sys_clk_n_i LOC = K16;
NET  sys_clk_n_i IOSTANDARD = LVDS_25;

NET "sys_clk_p_i" TNM_NET = sys_clk_p_i;
TIMESPEC TS_sys_clk_p_i = PERIOD "sys_clk_p_i" 200 MHz HIGH 50% INPUT_JITTER 50 ps;

